.PRECIOUS: $(CMDSTAN_HOME)bin/print$(EXE)
$(CMDSTAN_HOME)bin/print$(EXE) : $(CMDSTAN_HOME)bin/cmdstan/print.o
	@mkdir -p $(dir $@)
	$(LINK.c) -O$(O_STANC) $(OUTPUT_OPTION) $< $(LDLIBS)

.PRECIOUS: $(CMDSTAN_HOME)bin/stanc$(EXE)
$(CMDSTAN_HOME)bin/stanc$(EXE) : $(CMDSTAN_HOME)bin/cmdstan/stanc.o $(CMDSTAN_HOME)bin/libstanc.a
	@mkdir -p $(dir $@)
	$(LINK.c) -O$(O_STANC) $(OUTPUT_OPTION) $< $(LDLIBS_STANC)

